    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.4",
    "express": "^4.16.2",
    "file-loader": "^1.1.11",
    "font-awesome": "^4.7.0",
    "jest": "^23.3.0",
    "jquery": "^3.3.1",
    "jsonwebtoken": "^8.1.0",
    "moxios": "^0.4.0",
    "nightwatch": "^0.9.21",
