{
  "name": "event-manager",
  "version": "1.0.0",
  "description": "Event manager application",
  "scripts": {
    "start:dev": "nodemon --watch server/ --exec babel-node server/app.js",
    "test": "NODE_ENV=test npm run migrations && NODE_ENV=test npm run start:test && npm run test:react",
    "migrations": "sequelize db:migrate:undo:all && sequelize db:migrate",
    "start:test": "nyc --reporter=html --reporter=text mocha --timeout 250000 ./server/tests/index.js --compilers js:babel-register",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "coveralls": "nyc --reporter=lcov --reporter=text-lcov npm test",
    "client": "webpack-dev-server --config ./webpack.dev.config.js --progress",
    "start": "node ./server/dist/app.js",
    "build": "babel server -d server/dist --copy-files --ignore server/dist && webpack -p --config ./webpack.prod.config.js --progress",
    "postinstall": "npm run build",
    "test:api": "NODE_ENV=test npm run migrations && NODE_ENV=test npm run start:test",
    "test:react": "jest /client/* -u && codecov",
    "test:e2e": "nightwatch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nzediegwu1/EventManager.git"
  },
  "keywords": [
    "events",
    "facility",
    "centers",
    "locations",
    "fullstack"
  ],
  "author": "Anaeze Nsoffor",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nzediegwu1/EventManager/issues"
  },
  "homepage": "https://eventmanageronline.herokuapp.com",
  "dependencies": {
    "axios": "^0.17.1",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.2",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "babel-register": "^6.26.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.2",
    "bootstrap": "^4.0.0",
    "brotli-webpack-plugin": "^1.0.0",
    "cloudinary": "^1.10.0",
    "codecov": "^3.0.4",
    "compression-webpack-plugin": "^1.1.11",
    "connect-history-api-fallback": "^1.5.0",
    "cors": "^2.8.4",
